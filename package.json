{
  "name": "curso-webpack",
  "version": "1.0.0",
  "description": "esto es un curso muy divertido",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack index.js -o bundle.js",
    "build:local": "webpack",
    "build:external": "webpack --config ./external/webpack.config.js",
    "build:css": "webpack --config ./css-style-loader/webpack.config.js",
    "build:extract:css": "webpack --config ./plugin-extract-text/webpack.config.js",
    "build:multiple": "webpack --config ./multiple-entry-points/webpack.config.js",
    "build:dev": "webpack-dev-server --config ./webpack-dev-server/webpack.config.js",
    "build:babel": "webpack --config ./babel-loader/webpack.config.js",
    "build:images": "webpack --config ./url-loader-images/webpack.config.js",
    "build:fonts": "webpack --config ./url-loader-fonts/webpack.config.js",
    "build:videos": "webpack --config ./file-loader-videos/webpack.config.js",
    "build:json": "webpack --config ./json-loader/webpack.config.js",
    "build:react": "webpack --config ./react/webpack.config.js",
    "build:postcss": "webpack --config ./postcss-loader/webpack.config.js",
    "build:prevent": "webpack --config ./prevent-duplication/webpack.config.js",
    "build:vendor": "webpack --config ./vendor/webpack.config.js",
    "build:dll": "webpack --config ./dll/webpack.dll.config.js",
    "build:dll:src": "webpack --config ./dll/webpack.config.js",
    "build:dynamic": "webpack --config ./dynamic-imports/webpack.config.js"
  },
  "keywords": [
    "webpack"
  ],
  "author": "Edson J Vargas <edson.jhair@yahoo.com> (www.edsonvargas.com.ve)",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.11",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "i": "^0.3.6",
    "json-loader": "^0.5.7",
    "postcss": "^6.0.22",
    "postcss-cssnext": "^3.1.0",
    "postcss-loader": "^2.1.5",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.8.3",
    "webpack-cli": "^2.1.3",
    "webpack-dev-server": "^3.1.4"
  },
  "dependencies": {
    "react": "^16.3.2",
    "react-dom": "^16.3.2"
  }
}
